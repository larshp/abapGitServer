{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "useApackDependencies": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [
    {
      "url": "https://github.com/abaplint/deps",
      "folder": "/deps",
      "files": "/src/**/*.*"
    },
    {
      "url": "https://github.com/abapGit/abapGit",
      "folder": "/abapGit",
      "files": "/src/**/*.*"
    },
    {
      "url": "https://github.com/larshp/ABAP-Swagger",
      "folder": "/swag",
      "files": "/src/**/*.*"
    }
  ],
  "syntax": {
    "version": "v740sp08",
    "errorNamespace": "^(Z|Y)"
  },
  "rules": {
    "align_parameters": false,
    "cds_parser_error": true,
    "classic_exceptions_overlap": true,
    "constant_classes": true,
    "cyclic_oo": false,
    "dangerous_statement": true,
    "db_operation_in_loop": false,
    "identical_descriptions": false,
    "local_testclass_consistency": true,
    "many_parentheses": true,
    "max_one_method_parameter_per_line": true,
    "modify_only_own_db_tables": true,
    "no_aliases": false,
    "no_chained_assignment": true,
    "no_external_form_calls": true,
    "no_inline_in_optional_branches": false,
    "no_yoda_conditions": false,
    "nrob_consistency": true,
    "omit_preceding_zeros": true,
    "pragma_style": false,
    "prefer_corresponding": false,
    "prefer_raise_exception_new": false,
    "select_add_order_by": false,
    "select_performance": false,
    "slow_parameter_passing": false,
    "static_call_via_instance": true,
    "unnecessary_chaining": false,
    "unnecessary_pragma": true,
    "unsecure_fae": false,
    "call_transaction_authority_check": true,
    "function_module_recommendations": false,
    "intf_referencing_clas": true,
    "method_implemented_twice": true,
    "method_overwrites_builtin": false,
    "omit_parameter_name": true,
    "omit_receiving": true,
    "parser_702_chaining": true,
    "sy_modification": true,
    "uncaught_exception": true,
    "unused_methods": false,
    "identical_contents": true,
    "prefer_xsdbool": true,
    "check_subrc": false,
    "cyclomatic_complexity": true,
    "forbidden_pseudo_and_pragma": true,
    "identical_conditions": true,
    "line_break_style": false,
    "prefer_is_not": false,
    "unused_types": true,
    "use_bool_expression": true,
    "use_line_exists": false,
    "unknown_types": true,
    "begin_single_include": true,
    "downport": false,
    "unused_ddic": true,
    "line_break_multiple_parameters": false,
    "names_no_dash": true,
    "parser_missing_space": true,
    "prefer_inline": false,
    "reduce_string_templates": true,
    "forbidden_identifier": false,
    "forbidden_void_type": true,
    "try_without_catch": true,
    "abapdoc": false,
    "allowed_object_naming": true,
    "allowed_object_types": true,
    "use_class_based_exceptions": true,
    "ambiguous_statement": false,
    "begin_end_names": true,
    "chain_mainly_declarations": true,
    "check_abstract": true,
    "check_comments": false,
    "check_ddic": true,
    "check_include": true,
    "check_syntax": true,
    "check_text_elements": true,
    "check_transformation_exists": true,
    "form_tables_obsolete": true,
    "fully_type_constants": true,
    "implement_methods": true,
    "keep_single_parameter_on_one_line": true,
    "keyword_case": true,
    "main_file_contents": true,
    "newline_between_methods": true,
    "prefer_returning_to_exporting": true,
    "prefix_is_current_class": true,
    "release_idoc": true,
    "rfc_error_handling": true,
    "selection_screen_naming": true,
    "sicf_consistency": true,
    "sql_escape_host_variables": false,
    "type_form_parameters": true,
    "types_naming": true,
    "unused_variables": true,
    "xml_consistency": true,
    "7bit_ascii": true,
    "avoid_use": {
      "define": false,
      "testSeams": true,
      "statics": true,
      "describeLines": true,
      "exportToMemory": true,
      "exportToDatabase": true,
      "break": true,
      "defaultKey": false
    },
    "class_attribute_names": {
      "ignoreExceptions": true,
      "ignoreLocal": false,
      "constants": "",
      "ignoreInterfaces": false,
      "statics": "^G._.*$",
      "instance": "^M._.*$"
    },
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": true,
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": true,
    "description_empty": true,
    "double_space": {
      "keywords": true,
      "startParen": true,
      "afterColon": true,
      "endParen": true
    },
    "empty_line_in_statement": {
      "allowChained": false
    },
    "empty_statement": true,
    "empty_structure": {
      "loop": true,
      "if": true,
      "when": true,
      "while": true,
      "loopAllowIfSubrc": false,
      "case": true,
      "try": true,
      "select": false,
      "do": true,
      "at": true
    },
    "exit_or_check": true,
    "exporting": true,
    "functional_writing": {
      "ignoreExceptions": true
    },
    "global_class": true,
    "identical_form_names": true,
    "if_in_if": true,
    "in_statement_indentation": false,
    "indentation": {
      "ignoreExceptions": true,
      "alignTryCatch": false,
      "globalClassSkipFirst": false,
      "selectionScreenBlockIndentation": false,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "inline_data_old_versions": true,
    "line_length": {
      "length": 120
    },
    "line_only_punc": {
      "ignoreExceptions": true
    },
    "local_class_naming": {
      "local": "^LCL_.*$",
      "test": "^LTCL_.*$",
      "exception": "^LCX_.*$"
    },
    "local_variable_names": {
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": true,
    "message_exists": true,
    "method_length": {
      "errorWhenEmpty": true,
      "checkForms": true,
      "ignoreTestClasses": false,
      "statements": 100
    },
    "method_parameter_names": {
      "ignoreExceptions": true,
      "importing": "^I._.*$",
      "returning": "^R._.*$",
      "changing": "^C._.*$",
      "exporting": "^E._.*$",
      "ignoreNames": [
        "P_TASK"
      ]
    },
    "mix_returning": true,
    "msag_consistency": true,
    "nesting": {
      "depth": 5
    },
    "no_public_attributes": {
      "allowReadOnly": false,
      "ignoreTestClasses": false
    },
    "object_naming": {
      "clas": "^ZC(L|X)\\_",
      "intf": "^ZIF\\_",
      "prog": "^Z",
      "fugr": "^Z",
      "shlp": "^Z",
      "tabl": "^Z",
      "ttyp": "^Z",
      "dtel": "^Z",
      "doma": "^Z",
      "msag": "^Z",
      "tran": "^Z",
      "enqu": "^EZ",
      "auth": "^Z",
      "idoc": "^Z",
      "xslt": "^Z",
      "ssfo": "^Z",
      "ssst": "^Z",
      "pinf": "^Z"
    },
    "obsolete_statement": {
      "refresh": true,
      "compute": true,
      "occurences": true,
      "fieldSymbolStructure": true,
      "clientSpecified": true,
      "formDefinition": true,
      "formImplementation": true,
      "add": true,
      "parameter": true,
      "callTransformation": true,
      "communication": true,
      "exitFromSQL": true,
      "freeMemory": true,
      "pack": true,
      "ranges": true,
      "regex": false,
      "selectWithoutInto": true,
      "sortByFS": true,
      "typePools": true,
      "load": true,
      "subtract": true,
      "withHeaderLine": true,
      "multiply": true,
      "move": true,
      "divide": true,
      "requested": true,
      "occurs": true,
      "setExtended": true
    },
    "parser_error": true,
    "preferred_compare_operator": {
      "badOperators": [
        "EQ",
        "><",
        "NE",
        "GE",
        "GT",
        "LT",
        "LE"
      ]
    },
    "remove_descriptions": {
      "ignoreExceptions": false,
      "ignoreWorkflow": false
    },
    "sequential_blank": {
      "lines": 4
    },
    "short_case": {
      "length": 1,
      "allow": []
    },
    "space_before_colon": true,
    "space_before_dot": {
      "ignoreGlobalDefinition": true,
      "ignoreExceptions": true
    },
    "start_at_tab": true,
    "superclass_final": true,
    "tabl_enhancement_category": true,
    "unreachable_code": true,
    "use_new": false,
    "when_others_last": true,
    "whitespace_end": true
  }
}
